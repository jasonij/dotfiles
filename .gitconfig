[user]
	name = Jason Kroll
	email = jkroll@alumni.tufts.edu
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[alias]
	cp = cherry-pick
	st = status
	cl = clone
	ci = commit
	co = checkout
	br = branch
	diff = diff --word-diff
	dc = diff --cached
[push]
	default = simple
[diff]
	tool = vimdiff
