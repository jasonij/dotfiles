!URxvt*font: xft:DejaVu Sans Mono:pixelsize=14:antialias=true:hinting=true
!URxvt*boldFont: xft:DejaVu Sans Mono:bold:pixelsize=14:antialias=true:hinting=true

URxvt*font: xft:Inconsolata:size=13:antialias=true:hinting=true
URxvt*boldFont: xft:Inconsolata:bold:size=13:antialias=true:hinting=true

! URxvt*transparent: true
! URxvt*tintColor: #203050
! URxvt*shading: 63 # 1.0 - 1.0 / Math::E

URxvt.cursorBlink: 0

! These are used in ~/.tmux.conf  C-Tab, C-S-Tab
URxvt*keysym.C-Tab: \033[27;5;9~
URxvt*keysym.C-ISO_Left_Tab: \033[27;6;9~

URxvt*iso14755: false

URxvt.depth: 32

URxvt.saveLines:32767
URxvt.scrollBar: false

URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: uzbl-browser
URxvt.matcher.button: 2
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

